# Quantum-Hybrid DEX Accelerator - Environment template
# Copy to .env and fill in values

# Pharos Testnet (AtlanticOcean)
PHAROS_RPC_URL=https://atlantic.ocean.pharos.network
# Optional: QuickNode or other provider
# PHAROS_RPC_URL=https://pharos-testnet.quiknode.pro/YOUR_KEY/

# For signing transactions (optional, demo only)
# PRIVATE_KEY=0x...

# Redis (default for docker-compose)
REDIS_URL=redis://localhost:6379

# PostgreSQL (default for docker-compose)
DATABASE_URL=postgresql+asyncpg://qhda:qhda@localhost:5432/qhda

# IBM Qiskit (for real quantum hardware, optional)
# QISKIT_TOKEN=your_ibm_quantum_token

# D-Wave (for real annealer, optional)
# DWAVE_API_TOKEN=your_dwave_token

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_PHAROS_RPC=https://atlantic.ocean.pharos.network
